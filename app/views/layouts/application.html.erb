<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= render 'layouts/rails_default' %>
    <%= render 'layouts/shim' %>

  </head>
  <body>
  <div class="jumbotron">
    <div class="center jumbotron">

    <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |m_t, m| %>
      <div class="alertalert<%= m_t %>"><%= m %></div>
      <% end %>
      <%= yield %>
      <%= render 'layouts/footer' %>
      <!--#debug(params) if Rails.env.development?-->
      </div>
      <%#= debug(params) if Rails.env.development? %>
    </div>
    </div>
  </body>
</html>